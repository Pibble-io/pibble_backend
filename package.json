{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "babel ./src --out-dir dist --copy-files && echo 'Done. Waitnig for connections.'",
    "cron": "babel ./src --out-dir dist --copy-files && echo 'Done. Waitnig for connections.' && node ./dist/bin/cron",
    "local_cron": "concurrently \"nodemon --exec babel-node ./src/bin/cron\"",
    "server:start": "cross-env PORT=4001 0x -o ./dist/bin/www",
    "dev": "concurrently \"cross-env PORT=4001 nodemon --exec babel-node ./src/bin/www\" \"babel-node ./src/bin/cron\"",
    "local": "concurrently \"cross-env PORT=4001 nodemon --exec babel-node ./src/bin/www\"",
    "localdebug": "concurrently \"cross-env PORT=4001 nodemon --exec babel-node ./src/bin/www --inspect\"",
    "start": "babel ./src --out-dir dist --copy-files && echo 'Done. Waitnig for connections.' && concurrently \"cross-env PORT=4001 node --max_old_space_size=8192 ./dist/bin/www\" \"node ./dist/bin/cron\"",
    "start_dev": "babel ./src --out-dir dist --copy-files && echo 'Done. Waitnig for connections.' && concurrently \"cross-env PORT=4011 node --max_old_space_size=8192 ./dist/bin/www\" \"node ./dist/bin/cron\"",
    "start_metric": "babel ./src --out-dir dist_promo --copy-files && echo 'Done. Waitnig for connections.' && concurrently \"cross-env PORT=4101 node --max_old_space_size=8192 ./dist_promo/bin/www_metric\"",
    "local_metric": "concurrently \"cross-env PORT=4101 nodemon --exec babel-node ./src/bin/www_metric\"",
    "db:setup": "babel-node ./src/bin/db",
    "db:migrate": "babel-node node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo": "babel-node node_modules/.bin/sequelize db:migrate:undo",
    "db:seed:all": "babel-node node_modules/.bin/sequelize db:seed:all",
    "thumbnails:create": "babel-node ./src/bin/thumbnails",
    "wallets:sync": "babel-node ./src/bin/wallets",
    "wallets:encrypt": "babel-node ./src/bin/encryptWallet",
    "categories:sync": "babel-node ./src/bin/categories",
    "airdrop": "babel-node ./src/bin/airdrop"
  },
  "dependencies": {
    "@sentry/node": "^5.2.0",
    "amqplib": "^0.5.2",
    "app-root-path": "^2.2.1",
    "aws-sdk": "^2.259.1",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt-nodejs": "^0.0.3",
    "bitcoin-core": "^2.0.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "bunyan-express-serializer": "^1.0.0",
    "caver-js": "^1.0.0",
    "clarifai": "^2.9.0",
    "co-views": "^2.1.0",
    "concurrently": "^3.6.0",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.1.6",
    "csv-parser": "^2.2.0",
    "debug": "~4.1.1",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "exiftool": "^0.0.3",
    "express": "~4.16.0",
    "express-ejs-layouts": "^2.5.0",
    "express-fileupload": "^0.4.0",
    "express-oauth-server": "^2.0.0",
    "express-promise-router": "^3.0.2",
    "express-rate-limit": "^3.4.0",
    "express-request-id": "^1.4.1",
    "firebase-admin": "^7.0.0",
    "form-data": "^2.3.3",
    "i18n-2": "^0.7.3",
    "imghash": "^0.0.3",
    "jsonwebtoken": "^8.2.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "ms": "^2.1.1",
    "multer": "^1.3.0",
    "mysql2": "^1.5.3",
    "node-cron": "^2.0.3",
    "node-exiftool": "^2.3.0",
    "node-ffprobe": "^1.2.2",
    "nodemailer": "^4.6.6",
    "nodemon": "^1.17.5",
    "oauth2-server": "^3.0.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "randomstring": "^1.1.5",
    "revalidator": "^0.3.1",
    "s3-zip": "^2.0.1",
    "secure-pin": "^1.0.14",
    "sequelize": "4.44.2",
    "sequelize-cli": "^4.0.0",
    "sequelize-paginate": "^1.1.2",
    "simple-node-logger": "^18.12.22",
    "socket.io": "^2.2.0",
    "uuid": "^3.3.2",
    "validator": "^10.3.0",
    "wallet-address-validator": "^0.2.1",
    "web3": "1.0.0-beta.36",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0"
  }
}
